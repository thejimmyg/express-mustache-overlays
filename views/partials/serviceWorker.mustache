{{#serviceWorkerUrl}}
if ('serviceWorker' in navigator) {
  // The actual /sw.js is not served by express-mustache-overlays, you should write it yourself or use one generated by a tool like gateway-lite
  navigator.serviceWorker.register({{serviceWorkerUrl}})
  .then(function(reg) {
    // registration worked
    console.log('Registration succeeded. Scope is ' + reg.scope);
  }).catch(function(error) {
    // registration failed
    console.log('Registration failed with ' + error);
  });
}
{{/serviceWorkerUrl}}
