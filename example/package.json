{
  "name": "express-mustache-overlays-example",
  "version": "0.5.0",
  "description": "Example",
  "scripts": {
    "docker:build": "docker build . -t thejimmyg/express-mustache-overlays:0.5.0",
    "docker:push": "docker push thejimmyg/express-mustache-overlays:0.5.0",
    "docker:build:test": "docker build . -t thejimmyg/express-mustache-overlays:test",
    "docker:push:test": "docker push thejimmyg/express-mustache-overlays:test",
    "docker:run": "docker run -e SCRIPT_NAME='' -e DEBUG'=express-mustache-overlays:server,express-mustache-overlays' -e PORT=80 --init -p 80:80 thejimmyg/express-mustache-overlays:0.5.0",
    "fix": "standard --fix *.js",
    "start": "node server.js"
  },
  "author": "JimmyG",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "express-mustache-overlays": "file:.."
  },
  "devDependencies": {
    "standard": "^12.0.1"
  }
}
